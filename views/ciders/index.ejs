<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Birth<br>day</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% ciders.forEach(function(m) { %>
      <tr>
        <td><%= m.brewName %></td>
        <td><%= m.bDate %></td>
        <td><a href="/ciders/<%= m._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
